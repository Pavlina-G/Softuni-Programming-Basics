# -- 1 --

time1 = int(input())
time2 = int(input())
time3 = int(input())

total_time = time3 + time2 + time1
minutes = total_time // 60
seconds = total_time % 60

if seconds < 10:
    print(f'{minutes}:0{seconds}')
else:
    print(f'{minutes}:{seconds}')

# -- 2 --

time1 = int(input())
time2 = int(input())
time3 = int(input())

total_time = time3 + time2 + time1
minutes = total_time // 60
seconds = total_time % 60
print(f"{minutes}:{seconds:02d}")
